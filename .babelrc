{
  "presets": [
    ["env", {
      "loose": true,
      "useBuiltIns": true,
      "modules": false,
      "targets": {
        "browsers": [ "Ie > 8", "last 2 version", "safari >= 9" ]
      }
    }]
  ],
  "plugins": [
    [ "transform-class-properties", { "spec": true } ],
    [ "transform-object-rest-spread", { "useBuiltIns": true } ]
  ],
  "comments": true
}
